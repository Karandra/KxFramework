{
	"name": "kxf-framework",
	"version-string": "2.0.0",
	"port-version": 1,
	"supports": "windows",
	"description": "[master] Initially a support library for Kortex Mod Manager and a general use application framework",
	"homepage": "https://github.com/Karandra/kxf-framework",
	"license": "LGPL-3.0",
	"dependencies": [
		{
			"name": "wxwidgets",
			"default-features": false,
			"features": [
				"fonts",
				"media",
				"sound",
				"webview"
			]
		},
		{
			"name": "curl",
			"default-features": false,
			"features": [
				"non-http",
				"http2",
				"openssl"
			]
		},
		"sciter-js",
		"openssl",
		"openssl-windows",
		"websocketpp",
		"gumbo",
		"libffi",
		"lz4",
		"nlohmann-json",
		"simpleini",
		"tinyxml2",
		"scintilla",
		"xxhash",
		"7zip",
		"uriparser",
		"lunasvg",
		"detours"
	]
}