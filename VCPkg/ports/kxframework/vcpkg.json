{
    "name": "kxframework",
    "version-string": "2.0",
    "port-version": 1,
    "supports": "windows",
    "description": "[develop] Support library for Kortex Mod Manager",
    "homepage": "https://github.com/KerberX/KxFramework",
    "license": "LGPL-3.0",

    "dependencies":
	[
        "wxwidgets",
        "sciter",
        "openssl",
        "openssl-windows",
        "websocketpp",
        "gumbo",
        "libffi",
        "lz4",
        "nlohmann-json",
        "simpleini",
        "tinyxml2",
        "scintilla",
        "xxhash",
        "7zip",
        "uriparser",
		
		{
            "name": "curl",
            "default-features": false,
            "features":
			[
				"non-http",
				"http2",
				"openssl"
			]
        }
    ]
}
